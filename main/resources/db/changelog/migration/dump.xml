            <column name="hit_points" value="10000"/>
        </insert>

        <insert tableName="pets">
            <column name="name" value="Pikachu"/>
            <column name="type" value="Cat"/>
            <column name="color" value="White"/>
            <column name="hit_points" value="100012"/>
        </insert>

        <insert tableName="pets">
            <column name="name" value="Pikalo"/>
            <column name="type" value="Dog"/>
            <column name="color" value="Red"/>
            <column name="hit_points" value="1"/>
        </insert>
